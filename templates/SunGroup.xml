    <variable name="returns" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="*" value=" ** &#10;    * @author SunXianBing &#10;    * @email sunxianbing@purvar.com&#10;    * @date $date$ $time$ &#10;    * @describe X&#10;    */" description="object" toReformat="false" toShortenFQNames="true">
